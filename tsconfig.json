{
	"compilerOptions": {
		"module": "Node16",		
		"moduleResolution": "Node16",
		"target": "ES2023",
		"lib": [
            "ES2023"
        ],
		"outDir": "out",
		"resolveJsonModule": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true // don't know how to fix it
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		".vscode-test",
		"test-resources",
		".test-extensions"
	]
}
